{
  "version": "2.0",
  "extensions": {
    "durableTask": {
      "hubName": "workflowdemohub",
      "tracing": {
        "distributedTracingEnabled": true,
        "version": "V2"
      }
    }
  },
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      },
      "enableLiveMetricsFilters": true
    }
  }
}